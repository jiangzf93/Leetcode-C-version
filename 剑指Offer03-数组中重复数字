public class Solution {
    public int FindRepeatNumber(int[] nums) {
        for(int i=0;i<nums.Length;i++){
            if(nums[i]!=i){
                if(nums[nums[i]]==nums[i]){
                    return nums[i];
                }else{
                    int temp=nums[nums[i]];
                    nums[nums[i]]=nums[i];
                    nums[i]=temp;
                }
            }
        }
        return -1;
    }
}
